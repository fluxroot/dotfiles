set completion-query-items 40
set completion-ignore-case off
set meta-flag on
set input-meta on
set output-meta off
set convert-meta on
set show-all-if-ambiguous on
set bell-style none

"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": history-search-backward
"\e[6~": history-search-forward
"\e[3~": delete-char
"\e[2~": quoted-insert
"\e[C": forward-word
"\e[D": backward-word
